public class PrimeNumber {
public static void main(String[] args) {
	

	
	int number=29;
	
	boolean flag=true;
	
	for(int i=2; i <=number/2; i++ ) {
		
		if (number %2==0) {
			flag=false;
			break;
		}
	}
	if(flag) {
		System.out.println("it is prime");
	}
	
	
	
	
	
	
}
}